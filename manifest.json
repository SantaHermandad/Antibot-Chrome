{
  "manifest_version": 2,
  "name": "Antibot-Chrome",
  "version": "0.1",
  "description": "No Bots.",
  "icons": { "128": "icon_128.png" },
  "background": {
    "persistent": false
  },
  "permissions": ["http://*.google.com/", "https://*.google.com/"],
  "browser_action": {
    "default_title": "",
    "default_popup": "popup.html"
  }
}
